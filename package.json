{
  "name": "@trs/suit-toolkit",
  "version": "1.0.0",
  "description": "Suit is a VueJS UI framework that offers a boilerplated approach that attempts to easy development.",
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production"
  },
  "files": [
    "src/plugins/suit-toolkit/*"
  ],
  "exports": {
    ".": "./src/plugins/suit-toolkit/src/plugin.js",
    "./scss/*": "./src/plugins/suit-toolkit/scss/*",
    "./src/*": "./src/plugins/suit-toolkit/src/*"
  },
  "devDependencies": {
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "unplugin-vue-components": "^0.25.1",
    "vue": "^3.3.4",
    "vue-loader": "^17.2.2",
    "vue-router": "^4.2.2",
    "vue-template-compiler": "^2.7.14",
    "vuex": "^4.1.0",
    "laravel-mix": "^6.0.49"
  },
  "repository": {
    "type": "git",
    "url": "https://git.trscloud.net/trs/travis-cloud/suit-toolkit"
  },
  "author": "Travis Hughes",
  "license": "ISC",
  "publishConfig": {
    "@trs:registry": "https://git.trscloud.net/api/v4/projects/21/packages/npm/"
  }
}