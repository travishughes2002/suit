{
  "name": "@trs/trs-toolkit",
  "version": "2.0.0",
  "description": "TRS Toolkit is a UI Framework for creating frontend web interfaces for TRS Cloud. Implemented as a VueJS plugin for ease of use.",
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production"
  },
  "files": [
    "src/plugins/trs-toolkit/*"
  ],
  "exports": {
    ".": "./src/plugins/trs-toolkit/src/plugin.js",
    "./scss/*": "./src/plugins/trs-toolkit/scss/*",
    "./src/*": "./src/plugins/trs-toolkit/src/*"
  },
  "devDependencies": {
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "unplugin-vue-components": "^0.25.1",
    "vue": "^3.3.4",
    "vue-loader": "^17.2.2",
    "vue-router": "^4.2.2",
    "vue-template-compiler": "^2.7.14",
    "vuex": "^4.1.0",
    "laravel-mix": "^6.0.49"
  },
  "repository": {
    "type": "git",
    "url": "https://git.trscloud.net/trs/travis-cloud/slick-ui"
  },
  "author": "Travis Hughes",
  "license": "ISC",
  "publishConfig": {
    "@trs:registry": "https://git.trscloud.net/api/v4/projects/21/packages/npm/"
  }
}